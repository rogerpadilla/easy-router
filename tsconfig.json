{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "declarationDir": "dist",
    "lib": ["ES2019", "DOM"]
  },
  "exclude": ["node_modules", "dist", "coverage"]
}
